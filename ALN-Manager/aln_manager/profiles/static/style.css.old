a.resize_element {
	display: none;
}

* a.resize_element:hover{
	cursor: pointer;
}

body {
	background-color: #708090;
	width: 100%;
}

div#wrapper {
	background-image: url('/static/wrapper.jpg');
	background-repeat: repeat-y;
	//height: 1000px;
	margin: 0 auto;
	padding-bottom: 20px;
	width: 820px;
}

div#banner {
	background-image: url('/static/navbar.jpg');
	background-repeat: none;
	height: 116px;
	margin: 0 auto;
	width: 798px;
}

div#navbar {
	clear: both;
	height: 36px;
	margin: 0 auto;
	width: 770px;
}

div#navbar input {
	color: #8b8b8b;
	float: right;
	margin: 8px	20px 0 0;
}

 
 div#banner h1 {
	float: left;
	font-size: 24pt;
	padding: 10px 20px;
	text-shadow: #8b8b8b 3px 3px 3px;
}

div#banner div#title_description {
	clear: right;
	float: left;
	margin: 0 20px;
	padding: 10px 0 0 0;
}

div#banner p#title_description {
	float: left;
	font-size: 10pt;
	padding: 0 20px 20px;
}

div#content {
	border-top: 1px solid black;
	clear: both;
	margin: 0 40px;
	padding-top: 10px;
}

div#content h3 {
	float: left;
	padding: 10px 0;
}

div.table{
	clear: both;
	display: block;
}

div#content table{
	border-collapse:collapse;
	clear: both;
	width: 100%;
}

table th, td{
	font-weight: normal;
	padding-left: 5px;
	height: 25px;
	margin: 0;
	text-align: left;
	width: 33%;
}

table th {
	background-image: url('/static/table_head_gradient.jpg');
	background-repeat: none;
	border-left; 1px solid white;

}

td.number {
	text-align: center;
}

div#content form {
	clear: both;
}
